@model ContatoModel
@{
    ViewData["Title"] = "Detalhes do contato";
}

<div class="text-center">

    <h1 class="display-4">Detalhes sobre </h1>

</div>


        
    <div class="form-group">
        <label for="nome">Nome: @Model.Nome</label>
        
    </div>
    <br/>
    <div class="form-group">
        <label for="email">Email: @Model.Email</label>      
    </div>
    <br/>
    <div class="form-group">
        <label for="celular">Celular: @Model.Celular</label>   
    </div>
            
   
    <a class="btn btn-secondary" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>

